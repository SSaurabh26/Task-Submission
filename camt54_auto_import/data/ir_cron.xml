<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Scheduled Action for CAMT54 Auto Import -->
        <record id="cron_camt54_auto_import" model="ir.cron">
            <field name="name">CAMT54 Auto Import</field>
            <field name="model_id" ref="model_camt54_auto_importer"/>
            <field name="state">code</field>
            <field name="code">model.run_auto_import()</field>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>